{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "blazorwasm",
            "request": "launch",
            "hosted": true,
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/AuthenticationWithClientSideBlazor.Server/bin/Debug/net5.0/AuthenticationWithClientSideBlazor.Server.dll",
            "cwd": "${workspaceFolder}/src/AuthenticationWithClientSideBlazor.Server",
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "MYSQL_USERNAME": "rs",
                "MYSQL_PASSWORD": "rs",
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}